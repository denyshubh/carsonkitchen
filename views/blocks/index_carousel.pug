.container-fluid.p-0
	#foodCarousel.d-none.d-md-block.carousel.slide(data-ride="carousel")
		.carousel-inner
			.carousel-item.active
				img.d-block.img-fluid(src="img/alta/granola.jpg", alt="First slide" )
			.carousel-item
				img.d-block.img-fluid(src="img/alta/area-bar.jpg", alt="Second slide")
			.carousel-item
				img.d-block.img-fluid(src="img/alta/evento.jpg", alt="Third slide" )

		a.carousel-control-prev(href="#foodCarousel", role="button", data-slide="prev")
			span.carousel-control-prev-icon(aria-hidden="true")
			span.sr-only Previous
		a.carousel-control-next(href="#foodCarousel", role="button", data-slide="next")
			span.carousel-control-next-icon(aria-hidden="true")
			span.sr-only Next

	#foodCarouselMob.d-md-none
		.container-fluid(style="height:90vh; background-image:url('img/alta/area-bar.jpg');")