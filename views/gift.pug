extends layout

block content
    .container-fluid#page_content
        .container-fluid.mb-5
            .page-head.bg-center-cover-nr.py-5(style="background-image:url('https://images.unsplash.com/photo-1513507544439-d2cd3d79b274?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1500&q=80');")
                .page-head-heading.py-5
                    .container.text-center.py-5
                        h1.text-center.py-2(style="border-top: 1px white solid; border-bottom: 1px white solid;") Share Happiness
                .page-head-gradient(style="background-image:url('img/gradient-white-bottom-black-top.png')")
        .container-fluid
            .container.page-inner.py-5
                #group-page-text.page-container.align-center.mx-2.p-3
                    h1.text-center.pt-4.text-muted.content-heading Carssars Kitchen Gift Corner
                    p.lead.mt-3.content-body Online gift services is coming soon !!!
                    img.img-fluid.col-8.col-md-6(src = "img/burger-boeuf-seul.png")
