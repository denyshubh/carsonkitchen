extends layout

block content
	.container-fluid.p-0
		#foodCarousel.carousel.slide(data-ride="carousel")
			.carousel-inner(role="listbox", style=" width:100%; height: 600px !important;")
				.carousel-item.active
					img.d-block.img-fluid(src="img/alta/cafe.jpg", alt="First slide",style=". max-width: 100%,height:auto")
				.carousel-item
					img.d-block.img-fluid(src="img/alta/sushi.jpg", alt="Second slide", style=". max-width: 100%,height:auto" )
				.carousel-item
					img.d-block.img-fluid(src="img/alta/hamburguesa.jpg", alt="Third slide", style=". max-width: 100%,height:auto")

			a.carousel-control-prev(href="#foodCarousel", role="button", data-slide="prev")
				span.carousel-control-prev-icon(aria-hidden="true")
				span.sr-only Previous
			a.carousel-control-next(href="#foodCarousel", role="button", data-slide="next")
				span.carousel-control-next-icon(aria-hidden="true")
				span.sr-only Next
	
	include blocks/about
	include blocks/division 
	include blocks/menu
	include blocks/division 
	include blocks/reviews
